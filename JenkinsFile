pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main',
                    credentialsId: 'github_access_token',
                    url: 'https://github.com/ImNM/swe-jenkins.git'
            }
        }
        stage('ls command') {
            steps {
                sh 'ls -al'
            }
        }
        stage('mvn test') {
            steps {
                sh 'sudo mvn clean test'
            }
        }
    }
}